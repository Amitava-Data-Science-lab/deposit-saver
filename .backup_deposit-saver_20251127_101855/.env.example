# GCP Configuration
# Main Google Cloud Project for the application
GOOGLE_CLOUD_PROJECT=capstone-project-479414

# Google Cloud Location/Region for resources
GOOGLE_CLOUD_LOCATION=us-central1

# Google Cloud Storage
GCS_BANK_DATA_BUCKET=

# Vertex AI Configuration
VERTEX_AI_AGENT_ENGINE_ID=

# Enable Vertex AI for Google GenAI
GOOGLE_GENAI_USE_VERTEXAI=True

# Google API Key for Vertex AI Express Mode (Secret Manager path)
# This key is used for express mode in VertexAiSessionService
GOOGLE_API_KEY_SECRET=projects/485422000664/secrets/DEMO-ACCOUNT_API-KEY

# Firebase Configuration
# Project ID where the Firebase secret is stored in Secret Manager
FIREBASE_SECRET_PROJECT_ID=485422000664

# Name of the secret in Google Cloud Secret Manager containing Firebase config JSON
FIREBASE_SECRET_NAME=capstone-project-firebase-config

# Version of the secret to retrieve (latest, or a specific version number)
FIREBASE_SECRET_VERSION=latest

# Backend Configuration
# Base URL for your backend API (used for JWT token exchange)
BACKEND_URL=

# Optional: Direct Firebase API Key Override
# Only use this for development/testing. In production, use Secret Manager.
# FIREBASE_API_KEY=your-firebase-api-key-here
